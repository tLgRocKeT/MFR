CMD:dcp(playerid, params[], 0, 0)
{
	// Csak a játékosoknak nézzük a szintjét
	if(GetPVarInt(playerid, "Level") < 1 && !IsPlayerAdmin(playerid))
	{
		CHECK_SZINT(playerid, SZINT_BUILDRACE);
	}

	if(RaceBuilt != playerid) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_BUILD_ERROR"));
	if(BCurrentCheckpoints < 1) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_HIBA_NINCS_OLYAN_ELLENORZOPONT_1"));

	for(new i = BSelectedCheckpoint; i != BCurrentCheckpoints; i++)
	{
	    SetGVarFloat("BRaceCPX", X, i + 1);
	    SetGVarFloat("BRaceCPY", Y, i + 1);
	    SetGVarFloat("BRaceCPZ", Z, i + 1);
	}
	BSelectedCheckpoint--;
	if(--BCurrentCheckpoints < 1)
	{
		DisablePlayerRaceCheckpoint(playerid);
		BSelectedCheckpoint = 0;
		return 1;
	}
	else if(BSelectedCheckpoint < 0)
	{
		BSelectedCheckpoint = 0;
	}
	BActiveCP(playerid, BSelectedCheckpoint);
	SendClientMessage(playerid, COLOR_GREEN, LANG(playerid, "MSG_ELLENORZOPONT_TOROLVE"));

	AdminCommandLog("DCP", playerid, -1);
	return 1;
}

