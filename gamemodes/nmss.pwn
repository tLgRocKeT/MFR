	{
 		if(!GetPVarType(i, "AFKTime") && !(g_PlayerData[i] & e_AFK)) continue;

		datez ++;
	}

	if(datez)
	{
		SendClientMessagef(playerid, COLOR_GREEN, LANG(playerid, "MSG_AFKBAN_LEVO_JATAKOSOK_SZAMA"), datez);
