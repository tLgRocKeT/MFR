{
	printf("OnPlayerCaptureBlock playerid = %d, blockid = %d", playerid, blockid);
	if (HasPlayerSigned(playerid))
	{
        if (g_game_status == IN_PROGRESS)
		{
		    if (GetBlockUniqueID(blockid) != playerid)
	    	{
		    	SetBlockColor(blockid, p_has_color[playerid]);
