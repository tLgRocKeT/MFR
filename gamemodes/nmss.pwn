		default: return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_HIBA_ROSSZ_TIPUS_TIPUSOKERT"));
	}

    if(bID)
    {
        mysql_format(1, query, sizeof(query), "SELECT `raceid`, `race` FROM `racedata` WHERE `%s` = %d ORDER BY `raceid` LIMIT %d, 150", name, strval(szPlayer), --iLimit * 150);
