CMD:cmds(playerid, params[], 0, 0)
{
	new
		id, i, count;
	if(sscanf(params, "D(1)", id)) return SendUsage(playerid, "/parancslista <ID>");
    SetPVarInt(playerid, "ParancsID", id);

	id--;
	new
	    File:fCMD = fopen("/NMSS/commands.txt", io_read),
	    bool:bChange = true;
	dline[0] = EOS;
	
	if(!fCMD) return SendClientMessage(playerid, COLOR_RED, "HIBA: Fopen fail");
	while(fread(fCMD, i_str))
	{
		// Amíg nem értünk el oda ahonnan olvassuk, akkor addig megyünk
		if(i++ < (id * 30)) continue;

		if(i_str[0] == 149)
		{
			strcat(dline, "{A9C4E4}");
			bChange = true;
		}
		else
		{
			if(bChange)
			{
				strcat(dline, "{FF0000}");
				bChange = false;
			}
		}

		strcat(dline, i_str);
		if(++count == 30) break;
	}
	fclose(fCMD);

	g_pSelectTDFlags[playerid] |= e_SELECT_CMDLIST;
	TextDrawShowForPlayer(playerid, TD_CMDListAlatt[0]);
	TextDrawShowForPlayer(playerid, TD_CMDListAlatt[1]);
	SelectTextDraw(playerid, 0xFF0000AA);
	ShowPlayerDialog(playerid, CMDMENU, DIALOG_STYLE_MSGBOX, "Parancslista", dline, "Bezárás");
	return 1;
}

