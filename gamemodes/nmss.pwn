CMD:stele(playerid, params[], 4, 0)
{
	if(sscanf(params, "s[" #STR_SIZE "]s[" #STR_SIZE "]", name, i_str)) return SendUsage(playerid, "/stele <Parancs> <Teleport Név>");
	if(strlen(i_str) > 45) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_ROSSZ_HOSSZUSAG_MAXIMUM_45"));

    fcheck("NMSS/Teles.txt");
    new
		File:TeleFile = fopen("NMSS/Teles.txt", io_append);
	if(!TeleFile) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_EZT_A_PARANCSOT_JELENLEG_NEM_1"));

	GetPlayerPos(playerid, X, Y, Z);
	GetPlayerFacingAngle(playerid, Angle);
	format(tmp, 64, "CMD:%s(playerid, params[], 0, 0)\n", name);

	fwrite(TeleFile, tmp);
    fwrite(TeleFile, "{\n");

	format(tmp, 128, "\tNormalTeleEx(playerid, \"%s\", \"%s\", %.4f, %.4f, %.4f, %.4f);\n", i_str, name[1], X, Y, Z, Angle);
	fwrite(TeleFile, tmp);

    fwrite(TeleFile, "\treturn 1;\n");
    fwrite(TeleFile, "}\n\n");
	fclose(TeleFile);
	
	SendClientMessagef(playerid, COLOR_GREENYELLOW, LANG(playerid, "MSG_TELEPORT_ELMENTVE_"), name);
	AdminCommandLog("STELE", playerid, -1, i_str);
	return 1;
