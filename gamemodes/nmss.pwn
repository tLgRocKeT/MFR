stock IsBlack(color)
{
	new
	    r = color >>> 24,
		g = color >>> 16 & 0xFF,
		b = color >> 8 & 0xFF,
		a = color & 0xFF;
	return ((r < 10 && g < 10 && b < 10) || a < 16);
}

stock ShiftRGBAToARGB(color)
	return color >>> 8 | color << 24; //(color & 0xFF) << 24 | (color >>> 24 & 0xFF) << 16 | (color >>> 16 & 0xFF) << 8 | (color >>> 8 & 0xFF);
	
