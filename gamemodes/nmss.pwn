	if(!(g_pTDFlags[playerid] & e_NOTD_DMKILLS) && g_pDeathMatchID[playerid] != -1)
	{
		PlayerTextDrawShow(playerid, InDMKills[playerid]);
	}
	
	// Fallout textdraw
