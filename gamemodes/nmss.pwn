PUBLIC: MinigameReset()
{
    printf("MinigameReset");
	if (g_game_status != NOT_STARTED)
	{
		if (Iter_Count(Pandemonium) > 0)
		{
		    if(g_game_status != SIGN_UP_TIME)
			{
			    foreach(new i : Pandemonium)
				{
				    GameTextForPlayer(i, " ", 10, 3);
				    TogglePlayerControllable(i, true);
				    SetPlayerHealth(i, 100);
					SpawnPlayer(i);
				}
			}
		}
		g_game_status = NOT_STARTED;
		Iter_Clear(Pandemonium);
		Arena_Destroy();
	}
}

