stock IsPlayerInHouseZone(playerid)
{
	new Float:x, Float:y, Float:z;
	GetPlayerPos(playerid, x, y, z);

	for(new i; i != sizeof(aInteriorZones); i++)
	{
		if(	x >= aInteriorZones[i][fPosKURVACIGANY][0] && x < aInteriorZones[i][fPosKURVACIGANY][3] &&
			y >= aInteriorZones[i][fPosKURVACIGANY][1] && y < aInteriorZones[i][fPosKURVACIGANY][4] &&
			z >= aInteriorZones[i][fPosKURVACIGANY][2] && z < aInteriorZones[i][fPosKURVACIGANY][5] &&
			GetPlayerInterior(playerid) == aInteriorZones[i][e_Interior])
		{
			return i + 1;
		}
	}
	return 0;
}

