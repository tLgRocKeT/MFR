stock endrace()
{
	new
	    i;
	for( ; i != LCurrentCheckpoint; i++)
	{
	    DeleteGVar("RaceCPX", i);
	    DeleteGVar("RaceCPY", i);
	    DeleteGVar("RaceCPZ", i);
