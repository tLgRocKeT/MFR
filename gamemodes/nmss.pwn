}

PUBLIC: MinigamePrepare()
{
    printf("MinigamePrepare");
    if (g_game_status == SIGN_UP_TIME)
	{
	    if (Iter_Count(Pandemonium) < MIN_SIGN_UPS)
	    {
	        SendClientMessageToAll(-1, "MSG_BLOCK_MINIGAME_NEM_KEZDODOTT");
	        MinigameReset();
	        return 0;
	    }
	    Arena_Create();

		new
		    Float:x,
		    Float:y,
		    Float:z,
	        j;

        foreach(new i : Pandemonium)
		{
		    p_has_color[i] = g_default_colors[j];
		    GetBlockPos(g_start_pos[j], x, y, z);
		    PlayerTele(i, x, y, z + 3.0, 0, g_facing_ang[j], 12);
		    SetPlayerHealth(i, 65535);
		    TogglePlayerControllable(i, false);
		    SetCameraBehindPlayer(i);
		    j++;
		}
		g_game_status = IN_PROGRESS;
		SetTimerEx("MinigameCountdown", 1000, 0, "i", 3);
		return 1;
	}
	return 0;
}

PUBLIC: MinigameCountdown(count)
{
    printf("MinigameCountdown count = %d", count);
	if (g_game_status == IN_PROGRESS)
	{
	    if (count > 0)
		{
			new
		        str[2];

			format(str, sizeof str, "%i", count);
		    foreach(new i : Pandemonium)
			{
			    if(IsPlayerInRangeOfPoint(i, 30.0, ARENA_POS))
		    	{
				    GameTextForPlayer(i, str, 2000, 3);
				    PlayerPlaySound(i, 1056, ARENA_POS);
				}
			}
			count--;
			SetTimerEx("MinigameCountdown", 1000, 0, "i", count);
		}
		else
		{
		    foreach(new i : Pandemonium)
			{
			    if(IsPlayerInRangeOfPoint(i, 30.0, ARENA_POS))
		    	{
				    GameTextForPlayer(i, "~g~START!", 2000, 3);
				    PlayerPlaySound(i, 1057, ARENA_POS);
				    TogglePlayerControllable(i, true);
				}
			}
			SetTimerEx("MinigameUpdate", 1000, 0, "i", RUN_TIME);

			for (new i; i < MAX_PLAYERS; i++)
		    {
		        if(HasPlayerSigned(i))
		        {
				    if(!IsPlayerInRangeOfPoint(i, 30.0, ARENA_POS))
				    {
						Iter_Remove(Pandemonium, i);
				    }
				}
			}
		}
	}
}

PUBLIC: MinigameUpdate(seconds)
{
    printf("MinigameUpdate seconds = %d", seconds);
	if (g_game_status == IN_PROGRESS)
	{
	    if (Iter_Count(Pandemonium) < 2)
	    {
         	SendClientMessageToAll(-1, "MSG_A_BLOCK_MINIGAME_BEFEJEZODOTT");
	        MinigameReset();
	        return 0;
	    }

	    new
			str[128],
			red, blue,
			green, yellow;

		blocks(i)
		{
		    if(GetBlockColor(i) == BLOCK_RED)
		    {
		        red++;
		    }
		    if(GetBlockColor(i) == BLOCK_BLUE)
		    {
		        blue++;
		    }
		    if(GetBlockColor(i) == BLOCK_GREEN)
		    {
		        green++;
		    }
		    if(GetBlockColor(i) == BLOCK_YELLOW)
		    {
		        yellow++;
		    }
		}

		if(seconds > 10)
		{
			format(str, sizeof str, "~n~~n~~n~~n~~n~~n~~n~~n~~n~ ~y~Hátralévõ Idõ: ~w~%d ~n~~r~] ~w~%03d ~b~] ~w~%03d ~g~] ~w~%03d ~y~] ~w~%03d", seconds, red, blue, green, yellow);
		}
		else
		{
		    format(str, sizeof str, "~n~~n~~n~~n~~n~~n~~n~~n~~n~ ~y~Hátralévõ Idõ: ~r~%d ~n~~r~] ~w~%03d ~b~] ~w~%03d ~g~] ~w~%03d ~y~] ~w~%03d", seconds, red, blue, green, yellow);
		}

		foreach(new i : Pandemonium)
		{
		    GameTextForPlayer(i, FixGameString(str), 5000, 3);
		}

		seconds--;
		if (seconds >= 0)
		{
			SetTimerEx("MinigameUpdate", 1000, 0, "i", seconds);
		}
	    else
		{
		    new
		        winner,
		        min_value = 0,
		        u_blocks[MAX_PLAYERS],
				bool:draw = false;

            blocks(j)
			{
			    if(GetBlockUniqueID(j) != -1)
			    {
			    	u_blocks[GetBlockUniqueID(j)]++;
			    }
			}
			foreach(new i : Pandemonium)
			{
			    if(u_blocks[i] > min_value)
			    {
			        winner = i;
			        min_value = u_blocks[i];
			    }
			    TogglePlayerControllable(i, false);
			}
			//Checking possibility of draw
			foreach(new i : Pandemonium)
			{
			    if(winner != i)
			    {
				    if(u_blocks[winner] == u_blocks[i])
				    {
				        draw = true;
				    }
				}
			}
			if(draw)
			{
			    SendClientMessageToAll(-1, "MSG_A_BLOCK_MINIGAME_BEFEJEZODOTT_1");
			}
			else
			{
			    new
		        	w_name[MAX_PLAYER_NAME];

		    	GetPlayerName(winner, w_name, MAX_PLAYER_NAME);
		    	SendClientMessageToAllf(-1, "MSG_A_BLOCK_MINIGAME_BEFEJEZODOTT_2", GetPlayerColor(winner) >>> 8, w_name, winner);
		    	GivePlayerMoneyEx(winner, 5000);
		    	GivePlayerScore(winner, 5);
			}
		    SetTimer("MinigameReset", 4000, 0);
		}
	}
	return 1;
}

stock SetPlayerVelocityForward(playerid, Float:distance)
{
    new
		Float:za,
		Float:xA,
		Float:yA,
		Float:xB,
		Float:yB,
		jumper;

    GetPlayerPos(playerid, xA, yA, za);
    jumper = CreateObject(18680, xA, yA, za - 1.5, 0.00, 0.00, 0.00, 100.00);

    foreach(new i : Pandemonium)
    {
        PlayerPlaySound(i, 1039, xA, yA, za);
    }

    xB = xA; yB = yA;
    GetPlayerFacingAngle(playerid, za);

    xA += (distance * floatsin(-za, degrees));
    yA += (distance * floatcos(-za, degrees));

    xB = xA - xB;
    yB = yA - yB;

    p_jumper[playerid] = true;
    SetPlayerVelocity(playerid, xB, yB, 0.30);
    SetTimerEx("ResetJumper", 2000, 0, "ii", playerid, jumper);
}

