    g_PlayerData[playerid] &= ~e_CLEARWORLD;

	Streamer_ToggleItemUpdate(playerid, STREAMER_TYPE_OBJECT, true);
    g_PlayerData[playerid] &= ~e_CLEARWORLD_NOOBJECTS;
}

