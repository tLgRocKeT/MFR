			case 1: ShowPlayerDialog(playerid, 5000, DIALOG_STYLE_LIST, !"{" #XCOLOR_GREEN "}Versenyek listája", "Üres", !"Start", !"Mégse");
		}
		return 1;
	}

	for( ; i != rows; i++)
	{
        cache_get_row_int(i, 0, datez);
		cache_get_row(i, 1, i_str);
		if(!i)
		{
		    format(dline, sizeof(dline), "%d. %s", datez, i_str);
		}
		else
		{
			format(dline, sizeof(dline), "%s\n%d. %s", dline, datez, i_str);
		}
	}

	switch(type)
	{
		case 0: ShowPlayerDialog(playerid, DIALOG_RACES + 789, DIALOG_STYLE_LIST, !"{" #XCOLOR_ORANGE "}Aranytojások listája", dline, !"Lehívás", !"Bezárás");
		case 1: ShowPlayerDialog(playerid, DIALOG_RACES, DIALOG_STYLE_LIST, !"{" #XCOLOR_GREEN "}Versenyek listája", dline, !"Start", !"Mégse");
	}
	return 1;
}

