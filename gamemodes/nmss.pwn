		bold,
		fontcolor,
		backcolor,
		textalignment;
	if(sscanf(params, "ds[" #STR_SIZE "]dD(90)S(Arial)[" #STR_SIZE "]D(24)D(1)N(0xFFFFFFFF)N(0)D(0)", objectid, i_str, materialindex, materialsize, fontface, fontsize, bold, fontcolor, backcolor, textalignment))
