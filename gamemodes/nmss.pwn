CMD:buildrace(playerid, params[], 0, 0)
{
	// Csak a játékosoknak nézzük a szintjét
	if(GetPVarInt(playerid, "Level") < 1 && !IsPlayerAdmin(playerid))
	{
		CHECK_SZINT(playerid, SZINT_BUILDRACE);
	}
	
	if(RaceBuilt == playerid) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_HIBA_JELENLEG_IS_VERSENYT_SZERKESZT"));
	if(GetPVarType(playerid, "RaceParticipant")) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_HIBA_MAR_CSATLAKOZTAL_A_VERSENYHEZ"));
	if(RaceBuilt != INVALID_PLAYER_ID) return SendClientMessagef(playerid, COLOR_RED, LANG(playerid, "MSG_MAR__SZERKESZT_EGY_VERSENYT"), GetPlayerColor(RaceBuilt) >>> 8, pName(RaceBuilt), RaceBuilt);

	if(sscanf(params, "D(0)F(16.0)D(15000)D(300)", BRaceType, BCPsize, BVersenyMoney, BVersenyIdo))
			return SendUsage(playerid, "/buildrace <Type (0 = alap, 1 = hajó, 2 = repülõ, 3 = gylaog)> <CPSize = 16.0> <Money = 15.000> <Time = 300 second>");

	if(!(0 <= BCPsize <= 32.0)) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_HIBAS_CP_MERET_00__320"));
	if(!(0 <= BVersenyIdo <= 2000)) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_HIBAS_VERSENY_IDO_0__2000"));
	if(!(0 <= BVersenyMoney <= 100000)) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_HIBAS_NYEREMENY_ERTEK_0__100000"));

	new
		szName[12];
	switch(BRaceType)
	{
		case 0: szName = "Alap";
		case 1: szName = "Hajó";
		case 2: szName = "Repülõ";
		case 3: szName = "Gyalog";
		default: return SendClientMessage(playerid, COLOR_LIGHTGREEN, LANG(playerid, "MSG_ROSSZ_TIPUS_0__3"));
	}

	SendClientMessagef(playerid, COLOR_GREEN, LANG(playerid, "MSG_VERSENYSZERKESZTES_ELKEZDVE"), szName);
	SendClientMessage(playerid, COLOR_ORANGE, LANG(playerid, "MSG_VERSENY_ADATOKKAL_KAPCSOLATOS"));
	SendClientMessage(playerid, COLOR_DEFAULT, LANG(playerid, "MSG_A_VERSENY_ELMENTESE_PEDIG_SAVERACE"));
	SendClientMessage(playerid, COLOR_DEFAULT, LANG(playerid, "MSG_MAJD_EZUTAN_CLEARRACE_ES_MEHET"));

	RaceBuilt = playerid;
	BCurrentCheckpoints = 0;

	AdminCommandLog("BUILDRACE", playerid, -1);
	return 1;
}

