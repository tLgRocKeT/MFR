	            FalloutOState{FalloutOSelected} = 0xFF;
                FalloutOSelected = -1;

                if(--FalloutOCount <= 0)
                {
					new
					    iWinnerCount,
					    iWinnerID = INVALID_PLAYER_ID;
					foreach(new i: Player)
