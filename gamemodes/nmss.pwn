	}

	if(!(g_pTDFlags[playerid] & e_NOTD_XP))
	{
		PlayerTextDrawShow(playerid, g_XPText[playerid]);
	}

	if(!(g_pTDFlags[playerid] & e_NOTD_XPCSIK))
