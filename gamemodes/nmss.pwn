	// Regisztráció idejének kiszámítása
	if(regdate == -1 && playerid != -1)
	{
		reg[0] = PlayerInfo[playerid][pRegYear]; reg[1] = PlayerInfo[playerid][pRegMonth]; reg[2] = PlayerInfo[playerid][pRegDay]; reg[3] = PlayerInfo[playerid][pRegHour]; reg[4] = PlayerInfo[playerid][pRegMinute]; reg[5] = PlayerInfo[playerid][pRegSecond];
	}
	else
	{
        date(laston, reg[2], reg[1], reg[0], reg[3], reg[4], reg[5]);
	}

	// Utoljára aktív
	if(laston == -1 && playerid != -1)
	{
		last[0] = PlayerInfo[playerid][pLastYear]; last[1] = PlayerInfo[playerid][pLastMonth]; last[2] = PlayerInfo[playerid][pLastDay]; reg[3] = PlayerInfo[playerid][pLastHour]; last[4] = PlayerInfo[playerid][pLastMinute]; last[5] = PlayerInfo[playerid][pLastSecond];
