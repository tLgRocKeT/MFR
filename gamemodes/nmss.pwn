	new
	    iCount;
	cache_get_row_int(0, 0, iCount);
	if(!iCount)
	{
    	SendClientMessagef(playerid, COLOR_LIGHTGREEN, LANG(playerid, "MSG_AZ__PARANCSOT_EGYSZER_SEM_HASZ"), command);
