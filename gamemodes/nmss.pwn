	}
	if(!IsValidDynamicObject(objectid)) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_HIBA_NEM_LETEZO_OBJECT_ID"));

	for(new i, j = strlen(i_str); i != j; i++)
