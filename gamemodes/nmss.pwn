	SendClientMessagef(playerid, COLOR_GREEN, LANG(playerid, "MSG_ELLENORZOPONT__LETREHOZVA_4F"), BCurrentCheckpoints, X, Y, Z);

    SetGVarFloat("BRaceCPX", X, BCurrentCheckpoints);
    SetGVarFloat("BRaceCPY", Y, BCurrentCheckpoints);
    SetGVarFloat("BRaceCPZ", Z, BCurrentCheckpoints);

	BSelectedCheckpoint = BCurrentCheckpoints;
	SetBRaceCheckpoint(playerid, BCurrentCheckpoints, -1);
	if(!BCurrentCheckpoints++)
	{
		if((vID = GetPlayerDynamicVehicleID(playerid)))
		{
			GetDynamicVehicleZAngle(vID, BRaceAngle);
		}
		else
		{
			GetPlayerFacingAngle(playerid, BRaceAngle);
		}
		//SendClientMessagef(playerid, -1, LANG(playerid, "MSG_BRACEANGLE_4F"), BRaceAngle);
	}

    //AdminCommandLog("CP", playerid, -1);
	return 1;
