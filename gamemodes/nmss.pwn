			{
				ConvertTime(var1, PlayerInfo[i][pLastActive] - g_rStartTick, TempTime[0], TempTime[1], TempTime[2]);
				format(i_str, 32, "versenyez ( %d:%02d.%d )", TempTime[0], TempTime[1], TempTime[2]);
