	}
	
	if((e_IN_DERBY | e_IN_FALLOUT | e_IN_DM | e_IN_DUEL | e_IN_TRUCKING | e_IN_RANGE | e_IN_RACE | e_RABOL | e_KOCSIT_LOP | e_IN_GANGWAR | e_IN_CTC | e_POSTAS | e_PIZZAS | e_FLAG_OWNER | e_BCAR_OWNER | e_NOTELE) & g_pGamesData[playerid])
