stock SetPlayerWeaponInfo(playerid, slot, weapon, ammo, inputtext[])
{
	if(GetPVarType(playerid, "InDuel"))
	{
		g_pDuelWeapon[playerid]{slot} = weapon;
	}
	else
	{
		new
			level;
		switch(weapon)
		{
			case WEAPON_GRENADE: level = 30;
			case WEAPON_ROCKETLAUNCHER: level = 40;
			case WEAPON_HEATSEEKER: level = 45;
			case WEAPON_FLAMETHROWER: level = 37;
			case WEAPON_MINIGUN: level = 50;
			default: level = 1;
		}
        CHECK_SZINT_EX(playerid, level);

		new
		    iPos;
		if((iPos = strfind(inputtext, ".", true)) != -1)
		{
			strdel(inputtext, iPos, iPos + 1);
			sscanf(inputtext, "'$'d", iPos);
			printf("price: %d, input: %s", iPos, inputtext);
			
			// VIP-nak 20%-al olcsóbbak a fegyverek
			if(g_pVIP[playerid])
				iPos = (iPos / 100) * 80;
				
			if(GetPlayerMoneyEx(playerid) < iPos) return SendClientMessagef(playerid, COLOR_RED, LANG(playerid, "MSG_HIBA_NINCS_ELEG_PENZED_LEGALABB_6"), iPos);
			GivePlayerMoneyEx(playerid, -iPos);
		}

		g_pWeaponSlot[playerid][slot] = weapon;
		g_pWeaponAmmo[playerid][slot] = ammo;
		GivePlayerWeapon(playerid, weapon, ammo);
	}
	return 1;
}

