		return 1;
	}
	if(iLimit < 1) return SendClientMessage(playerid, COLOR_RED, LANG(playerid, "MSG_INVALID_LIST_ID"));

	switch(iType)
	{
		case 1: name = "race";
		case 2: name = "holder";
		case 3: name = "regid", bID = true;
		case 4: name = "vehicle";
		case 5: name = "cpsize", bID = true;
